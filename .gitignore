# Ignore compiled Python files
__pycache__/
*.pyc
*.pyo
*.egg-info/
build/

# Ignore macOS system files
.DS_Store

# Ignore most ephemeris data (334MB total), but include essential files for 1800-2400 CE
/data/*
!/data/swisseph/
!/data/notables/
!/data/README.md

# Now ignore most of swisseph except what we need
/data/swisseph/*
!data/swisseph/ephe/
/data/swisseph/ephe/*

# Include essential ephemeris files for basic functionality (covers 1800-2400 CE, ~7.8MB total)
!data/swisseph/ephe/sepl_18.se1
!data/swisseph/ephe/sepl_24.se1
!data/swisseph/ephe/semo_18.se1
!data/swisseph/ephe/semo_24.se1
!data/swisseph/ephe/seas_18.se1
!data/swisseph/ephe/seas_24.se1
!data/swisseph/ephe/sefstars.txt

# Include notable births and events data (package assets)
!data/notables/births/
!data/notables/births/*.yaml
!data/notables/events/
!data/notables/events/*.yaml

# Ignore experimentation area
dev_experiments/

# Ignore the dist for now
dist/

# Ignore virtual environments
venv/
env/

# Ignore logs & temp files
*.log

# Ignore the caching system
.cache/

# Ignore coverage files
.coverage
coverage.json
htmlcov/

# Ignore development folder
dev/

# Sphinx docs output
docs/_build/